<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <template id="report_journal_entries">
    <t t-call="web.html_container">
      <t t-foreach="docs" t-as="doc">
        <t t-call="imw__customization.report_journal_entries_document" t-lang="doc.partner_id.lang"/>
      </t>
    </t>
  </template>

  <template id="report_journal_entries_document">
    <t t-call="web.external_layout">
      <t t-set="doc" t-value="doc.with_context({'lang':doc.partner_id.lang})"/>
      <t t-set="address">
        <div t-field="doc.partner_id"
             t-options="{&quot;widget&quot;: &quot;contact&quot;, &quot;fields&quot;: [&quot;address&quot;, &quot;name&quot;], &quot;no_marker&quot;: True}"/>
        <p t-if="doc.partner_id.vat">
          <t t-esc="doc.company_id.country_id.vat_label or 'Tax ID'"/>:
          <span t-field="doc.partner_id.vat"/>
        </p>
      </t>
       <div class="page">
        <div class="oe_structure"/>

        <h2>
          <p t-field="doc.date"/>
          <p t-field="doc.ref"/>
          <p t-field="doc.imw_ref2"/>
          <p t-field="doc.journal_id"/>
          <p t-field="doc.partner_id"/>
          <p t-field="doc.line_ids"/>
          <p t-field="doc.display_name"/>
          <p t-field="doc.currency_id"/>

        </h2>
    </div>

    </t>







  </template>

</odoo>